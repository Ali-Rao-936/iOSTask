# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'IOS Task' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for IOS Task

   pod 'Alamofire' #Webservice
   pod 'SwiftyJSON' #API mapping
   pod 'ProgressHUD' # Progress View
   pod 'IQKeyboardManagerSwift' # Keyboard avoider
   pod 'Kingfisher' # Image Caching and Loading
   pod 'ReachabilitySwift'  #To check if network available
   pod 'SwiftEntryKit' #Dialogs

  target 'IOS TaskTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'IOS TaskUITests' do
    # Pods for testing
  end

end
